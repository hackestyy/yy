<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping>

<class name="com.cssrc.bean.admin.Smemberresearchproduction" table="T_SMEMBERRESEARCHPRODUCTION">
    <id name="smrp_id" type="java.lang.Integer" column="SMRP_ID" unsaved-value = "null">
         <generator class="native"/>
         </id>
    <property name="smbi_id" type="java.lang.Integer" update="true" insert="true" column="SMBI_ID"/>
    <property name="prodname" type="java.lang.String" update="true" insert="true" column="PRODNAME"/>
    <property name="proddate" type="java.util.Date" update="true" insert="true" column="PRODDATE"/>
    <property name="prodrewards" type="java.lang.String" update="true" insert="true" column="PRODREWARDS"/>
    <property name="prodapplication" type="java.lang.String" update="true" insert="true" column="PRODAPPLICATION"/>
    </class>
</hibernate-mapping>
