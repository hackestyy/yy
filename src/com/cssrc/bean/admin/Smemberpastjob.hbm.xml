<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping>

<class name="com.cssrc.bean.admin.Smemberpastjob" table="T_SMEMBERPASTJOB">
    <id name="smpj_id" type="java.lang.Integer" column="SMPJ_ID" unsaved-value = "null">
         <generator class="native"/>
         </id>
    <property name="smbi_id" type="java.lang.Integer" update="true" insert="true" column="SMBI_ID"/>
    <property name="jobinstitute" type="java.lang.String" update="true" insert="true" column="JOBINSTITUTE"/>
    <property name="jobrank" type="java.lang.String" update="true" insert="true" column="JOBRANK"/>
    <property name="startdate" type="java.util.Date" update="true" insert="true" column="STARTDATE"/>
    <property name="enddate" type="java.util.Date" update="true" insert="true" column="ENDDATE"/>
    <property name="jobaddress" type="java.lang.String" update="true" insert="true" column="JOBADDRESS"/>
    </class>
</hibernate-mapping>
