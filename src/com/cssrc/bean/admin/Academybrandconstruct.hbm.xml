<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping>

<class name="com.cssrc.bean.admin.Academybrandconstruct" table="T_ACADEMYBRANDCONSTRUCT">
    <id name="abc_id" type="java.lang.Integer" column="ABC_ID" unsaved-value = "null">
         <generator class="native"/>
         </id>
    <property name="abi_id" type="java.lang.Integer" update="true" insert="true" column="ABI_ID"/>
    <property name="activename" type="java.lang.String" update="true" insert="true" column="ACTIVENAME"/>
    <property name="activeno" type="java.lang.String" update="true" insert="true" column="ACTIVENO"/>
    <property name="activememo" type="java.lang.String" update="true" insert="true" column="ACTIVEMEMO"/>
    </class>
</hibernate-mapping>
