<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping>

<class name="com.cssrc.bean.admin.Smemberacademicliterature" table="T_SMEMBERACADEMICLITERATURE">
    <id name="smal_id" type="java.lang.Integer" column="SMAL_ID" unsaved-value = "null">
         <generator class="native"/>
         </id>
    <property name="smbi_id" type="java.lang.Integer" update="true" insert="true" column="SMBI_ID"/>
    <property name="litename" type="java.lang.String" update="true" insert="true" column="LITENAME"/>
    <property name="litedate" type="java.util.Date" update="true" insert="true" column="LITEDATE"/>
    <property name="litejournal" type="java.lang.String" update="true" insert="true" column="LITEJOURNAL"/>
    <property name="litehouse" type="java.lang.String" update="true" insert="true" column="LITEHOUSE"/>
    </class>
</hibernate-mapping>
