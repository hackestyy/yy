<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping>

<class name="com.cssrc.bean.admin.Stemp" table="T_STEMP">
    <id name="id" type="java.lang.Integer" column="ID" unsaved-value = "null">
    	<generator class="increment">
        </generator>
    </id>
    <property name="username" type="java.lang.String" update="true" insert="true" column="USERNAME"/>
    <property name="stempname" type="java.lang.String" update="true" insert="true" column="STEMPNAME"/>
    
</class>
</hibernate-mapping>
